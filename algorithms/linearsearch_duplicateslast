# linear search (duplicates-find last occurence)
def linear_search(A, target):
  for i in range(len(A)-1, -1, -1):
      if A[i] == target: # found
        return i
  return -1 # not found


# main
data = [1, 2, 5, 2, 8, 8]
print(linear_search(data, 2)) # success
print(linear_search(data, 4)) # failure
